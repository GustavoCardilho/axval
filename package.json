{
  "name": "axval",
  "version": "1.0.0",
  "description": "string validation library",
  "main": "./dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "tsup src/index.ts --format cjs --dts-resolve --minify --clean --sourcemap --dts && node ./dist/index.js",
